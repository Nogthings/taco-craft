{
    "name": "tacocraft/tacocraft",
    "version": "1.0.1",
    "description": "Craft Laravel Projects with Mexican Flavor - A modern Laravel scaffolding tool",
    "keywords": [
        "laravel",
        "docker",
        "scaffolding",
        "generator",
        "mexico",
        "development",
        "php"
    ],
    "type": "project",
    "license": "MIT",
    "homepage": "https://tacocraft.dev",
    "authors": [
        {
            "name": "Oscar Galvez",
            "email": "oscargalvez812@gmail.com",
            "homepage": "https://github.com/oscargalvez",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/oscargalvez/tacocraft/issues",
        "source": "https://github.com/oscargalvez/tacocraft",
        "docs": "https://github.com/oscargalvez/tacocraft#readme"
    },
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "symfony/filesystem": "^6.0|^7.0",
        "symfony/finder": "^6.0|^7.0",
        "symfony/yaml": "^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "TacoCraft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/tacocraft"
    ],
    "scripts": {
        "test": "pest",
        "test:unit": "pest --group=unit",
        "test:feature": "pest --group=feature",
        "analyse": "phpstan analyse",
        "format": "pint",
        "format:test": "pint --test",
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}